#include<stdio.h>
int main()
{   
    int i,n,a[10],count1=0,count0=0;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
      scanf("%d",&a[i]);
    }    
    for(i=0;i<n;i++)
    {
        if(a[i]==1)
        {
            count1++;
        }
        else if(a[i]==0)
        {
            count0++;
        }
    }
    if(count1>count0)
    {
        printf("1");
    }
    else if(count1<count0)
    {
        printf("-1");
    }
    else
    {
        printf("0");
    }
}


